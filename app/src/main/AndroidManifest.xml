<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartsheba.smartforce">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OfferOtherActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".OfferHotelActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".OfferElectronicsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".OfferVehicleActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".OfferMedicalActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OfferDressActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ContactActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>